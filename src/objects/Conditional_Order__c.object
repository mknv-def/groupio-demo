    <validationRules>
        <fullName>VR_Order_Cancellation_Reason</fullName>
        <active>false</active>
        <description>Cancellation reason required</description>
        <errorConditionFormula>ISCHANGED(Status__c) &amp;&amp; 
ISPICKVAL(Status__c, &quot;Cancelled&quot;) &amp;&amp; 
ISBLANK(Cancellation_Reason__c)</errorConditionFormula>
        <errorDisplayField>Cancellation_Reason__c</errorDisplayField>
        <errorMessage>Please provide a cancellation reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Order_No_Edit_When_Confirmed</fullName>
        <active>true</active>
        <description>Cannot edit confirmed orders</description>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE(Status__c), &quot;Confirmed&quot;) &amp;&amp; 
NOT(ISPICKVAL(Status__c, &quot;Confirmed&quot;))</errorConditionFormula>
        <errorMessage>Confirmed orders cannot be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Order_Proposal_Active</fullName>
        <active>true</active>
        <description>Can only create orders for active proposals</description>
        <errorConditionFormula>ISNEW() &amp;&amp; NOT(ISPICKVAL(Group_Buy_Proposal__r.Status__c, &quot;Active&quot;))</errorConditionFormula>
        <errorDisplayField>Group_Buy_Proposal__c</errorDisplayField>
        <errorMessage>Orders can only be placed for Active proposals.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Order_Quantity_Positive</fullName>
        <active>true</active>
        <description>Quantity must be at least 1</description>
        <errorConditionFormula>Quantity__c &lt; 1</errorConditionFormula>
        <errorDisplayField>Quantity__c</errorDisplayField>
        <errorMessage>Order quantity must be at least 1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Order_Within_Available_Quota</fullName>
        <active>true</active>
        <description>Order quantity cannot exceed available quota</description>
        <errorConditionFormula>ISNEW() &amp;&amp; Quantity__c &gt; Group_Buy_Proposal__r.Available_Quota__c</errorConditionFormula>
        <errorDisplayField>Quantity__c</errorDisplayField>
        <errorMessage>Error Message: Order quantity exceeds available quota. Maximum available: {!Group_Buy_Proposal__r.Available_Quota__c}</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
